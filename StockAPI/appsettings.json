{
  "JwtSettings": {
    "Issuer": "StockManager",
    "Audience": "StockAPI",
    "AccessTokenExpirationMinutes": 60,
    "RefreshTokenExpirationDays": 30
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "server=localhost;port=3306;database=stockmanager_db;user={DB_USER};password={DB_PASSWORD}"
  }

}


